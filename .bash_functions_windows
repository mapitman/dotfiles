 dig () {
     winpty docker run -it --rm mapitman/linux-tools dig $@
 }
 host () {
     winpty docker run -it --rm mapitman/linux-tools host $@
 }

 pwgen () {
     winpty docker run -it --rm  mapitman/linux-tools pwgen "$@"
 }

 telnet () {
     winpty docker run -it --rm mapitman/linux-tools telnet "$@";
 }

 nc () {
     winpty docker run -it --rm mapitman/linux-tools nc "$@";
 }

 vs ()
 {
     files=(./*.sln)
     if [ -e ${files[0]} ]
     then
         start ${files[0]}
     else
         files=(./*.csproj)
         if [ -e ${files[0]} ]
         then
             start ${files[0]}
         fi
     fi
 }

authaws () {
    ~/src/work/mpitman/powershell-aws/AwsCli/bin/authaws $1
    export AWS_PROFILE="getty-$1"
    if [[ "$AWS_DEFAULT_REGION" != "" ]]; then 
        export AWS_DEFAULT_REGION="us-west-2"
    fi 
}

sudoaws() {
    existing_profile=$AWS_PROFILE
    ~/src/work/mpitman/powershell-aws/AwsCli/bin/sudoaws $1
    export AWS_PROFILE="$existing_profile-doer-$1"
}

